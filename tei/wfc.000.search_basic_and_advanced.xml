        <body>



            <div1 type="html">

                <h2>Search</h2>

                <div id="searchForm">


                    <ul>
                        <li>
                            <a href="#basic">Basic</a>
                        </li>
                        <li>
                            <a href="#advanced">Advanced</a>
                        </li>
                    </ul>
                    <div id="basic">



                        <form action="result.html" method="get" enctype="text/plain">
                            <div class="basicSearch1">
                                <h3>Search by keyword:</h3>


                                <input id="basic-q" type="text" name="q" value="" class="textField"/>

                                <p class="submit">
                                    <input type="submit" value="Search Documents" class="submit"/>
                                </p>

                                <p>Tips: <br/> To exclude a word, preceed with "-" (i.e. cody
                                    -buffalo)<br/> To search for a phrase, include in quotations
                                    (i.e. "bringing indians")</p>
                            </div>
                            <!-- basicSearch1-->

                        </form>

                        <div class="basicSearch2">
                            <p>or find:</p>

                            <div id="searchNounOption">


                                <ul>
                                    <li>
                                        <a href="#people">People</a>
                                    </li>
                                    <li>
                                        <a href="#places">Places</a>
                                    </li>
                                </ul>
                                <div id="people" class="namebox">

                                    <form action="people.html" method="get" enctype="text/plain">
                                        <input id="peopleSearch"
                                            class="peopleSearch peopleinput textField" type="text"
                                            name="q" value=""/>

                                        <p class="submit">
                                            <input type="submit" value="Go" class="submit"/>
                                        </p>
                                    </form>

                                </div>
                                <div id="places" class="namebox">

                                    <form action="places.html" method="get" enctype="text/plain">
                                        <input id="placesSearch"
                                            class="placesSearch placesinput textField" type="text"
                                            name="q" value=""/>

                                        <p class="submit">
                                            <input type="submit" value="Go" class="submit"/>
                                        </p>
                                    </form>


                                </div>
                                <!-- /#places-->
                            </div>
                            <!-- /#searchNounOption-->
                        </div>
                        <!-- /#basicSearch2-->





                    </div>
                    <!-- /#basic -->

                    <div id="advanced">

                        <form action="advanced.html" method="get" enctype="text/plain">
                            <div class="advancedSearch1">

                                <fieldset class="q">
                                    <label for="q">Keyword:</label>
                                    <input id="q" type="text" name="q" value=""
                                        class="textField"/>
                                    <p>Tips: To exclude a word, preceed with "-" (i.e. cody
                                        -buffalo), To search for a phrase, include in quotations
                                        (i.e. "bringing indians")</p>
                                </fieldset>
                                <h3>Limit by:</h3>
                                <fieldset class="limitBy">
                                    <label for="people">Person:</label>
                                    <input id="advancedPeople"
                                        class="peopleSearch peopleinput textField" type="text"
                                        name="people" value=""/>
                                </fieldset>
                                <fieldset class="limitBy">
                                    <label for="places">Place:</label>
                                    <input id="advancedPlaces"
                                        class="placesSearch peopleinput textField" type="text"
                                        name="places" value=""/>
                                </fieldset>

                                <fieldset class="limitBy"><label for="yearStart"> Year (<span id="exampleYearStart">1904</span>-<span id="exampleYearStop">1907</span>)</label>
                                    <input id="yearStart" type="text" size="4" name="yearStart" value=""
                                        class="textField"/> - <input id="yearStop" type="text" size="4"
                                        name="yearStop" value="" class="textField"/></fieldset>



                                <fieldset class="limitBy">
                                    <label for="author">Search only texts authored by William F.
                                        Cody</label>

                                    <input id="author" name="author" type="checkbox"
                                        value="Cody, William Frederick, 1846-1917"/>

                                </fieldset>

